<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top nav_site_bar">
        <div class="container">
            <div class="navbar-header">
                @Html.ActionLink("Amortization Calculator","Index","Home",new { area=""},new { @class="navbar-brand"})
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <footer>
            <div class="modal-footer">
                <p>&copy; @DateTime.Now.Year - Jaime Valencia</p>
            </div>
        </footer>
    </div>
</body>
</html>
